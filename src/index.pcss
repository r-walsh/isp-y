@font-face {
	font-family: 'Oswald';
	src: url('./assets/Oswald/Oswald-Regular.ttf') format('truetype'),
	url('./assets/Oswald/Oswald-Light.ttf') format('truetype'),
	url('./assets/Oswald/Oswald-Bold.ttf') format('truetype');
}

* {
	font-family: 'Oswald', sans-serif;
	letter-spacing: .13em;
}

html, body {
	margin: 0;
	padding: 0;
}
